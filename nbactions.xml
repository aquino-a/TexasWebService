<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath com.aquino.TexasWebService.TexasWebServiceApplication</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-build no test</actionName>
            <displayName>build no test</displayName>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
                <goal>-Dmaven.test.skip=true</goal>
            </goals>
        </action>
    </actions>
